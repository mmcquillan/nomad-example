job "worker" {
  type = "service"
  datacenters = ["dc1"]
  group "worker" {
    count = 1
    task "worker" {
      driver = "exec"
      config {
        command = "spew"
        args    = ["ABC"]
      }
      artifact {
        source = "https://github.com/mmcquillan/spew/releases/download/v0.1/spew.tar.gz"
      }
      env {
        SPEW_OUT = "testing nomad"
        SPEW_INTERVAL = "3s"
      }
    }
  }
}
